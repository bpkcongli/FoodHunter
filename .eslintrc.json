{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["jasmine"],
    "rules": {
        "require-jsdoc": 0,
        "max-len": 0,
        "new-cap": 0
    }
}
